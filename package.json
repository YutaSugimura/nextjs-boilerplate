{
  "name": "nextjs-boilerplate",
  "version": "3.2.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --pretty --noEmit",
