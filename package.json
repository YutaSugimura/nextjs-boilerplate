{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit",
    "test-all": "yarn type-check && yarn lint && yarn test",
    "lint": "next lint && prettier --check .",
    "format": "next lint && prettier --write .",
    "storybook": "start-storybook -s public -p 6006",
    "storybook:build": "build-storybook -s public",
    "prepare": "husky install"
  },
  "dependencies": {
    "autoprefixer": "^10.3.7",
    "next": "11.1.2",
    "postcss": "^8.3.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.0.1",
    "tailwindcss": "^2.2.16"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@storybook/addon-a11y": "^6.3.11",
    "@storybook/addon-actions": "^6.3.11",
    "@storybook/addon-essentials": "^6.3.11",
    "@storybook/addon-links": "^6.3.11",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.3.11",
    "@storybook/addon-viewport": "^6.3.11",
    "@storybook/react": "^6.3.11",
    "@testing-library/dom": "^8.7.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@types/react": "^17.0.27",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^27.2.4",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.1",
    "jest": "^27.2.4",
    "lint-staged": "^11.0.1",
    "msw": "^0.35.0",
    "msw-storybook-addon": "^1.4.1",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "storybook-addon-designs": "^6.2.0",
    "storybook-addon-pseudo-states": "^1.0.0",
    "typescript": "^4.4.3",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
