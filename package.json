    "babel-loader": "^8.2.3",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "eslint-plugin-storybook": "^0.5.3",
    "jest": "^27.4.3",
    "msw": "^0.36.3",
    "msw-storybook-addon": "^1.5.0",
    "postcss": "^8.4.5",
    "react-test-renderer": "^17.0.2",
    "storybook-addon-designs": "^6.2.0",
    "storybook-addon-pseudo-states": "^1.0.0",
    "tailwindcss": "^3.0.7",
    "typescript": "4.5.3",
    "whatwg-fetch": "^3.6.2",
    "husky": "^7.0.0"
  },
  "msw": {
    "workerDirectory": "public"
