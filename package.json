    "msw-storybook-addon": "^1.6.0",
    "postcss": "^8.4.12",
    "react-test-renderer": "^18.0.0",
    "storybook-addon-designs": "^6.2.1",
    "storybook-addon-pseudo-states": "^1.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.3",
    "whatwg-fetch": "^3.6.2",
    "husky": "^7.0.0"
